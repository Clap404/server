{
    "name": "php-mcp/stdio-sample",
    "description": "STDIO example for php-mcp",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "php-mcp/server": "@dev"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Test\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
} 